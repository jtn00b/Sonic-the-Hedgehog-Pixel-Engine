        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Camera Variables

    //Main values
    cam_x = obj_player.x;
    cam_y = obj_player.y;
    target = obj_player;
    center_x = global.Window_X/2
    center_y = global.Window_Y/2
    
    //Margin values
    margin_left = 16;
    margin_right = 0;
    margin_top = 32;
    margin_bottom = 32;
    
    //Bound values
    minimum_x = 0;
    maximum_x = room_width;
    minimum_y = 0;
    maximum_y = room_height;
    
    //Misc values
    shift_x = 0;
    shift_y = 0;
    ground_offset = 0;
    hor_lag = 0;
    ver_lag = 0;
    mode = 0;
    
    
</string>
