    <event eventtype="3" enumb="2">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Mode 0: Normal Scroll
// Horziontal Movement

if mode = 0 {
    if (target.x&gt;=cam_x+margin_right-shift_x &amp;&amp; hor_lag = 0 &amp;&amp; cam_x &lt; maximum_x-center_x){
        cam_x=min(cam_x+16,target.x-margin_right+shift_x);
    }
    
    if (target.x&lt;=cam_x-margin_left-shift_x &amp;&amp; hor_lag = 0 &amp;&amp; cam_x &gt; minimum_x+center_x){
        cam_x=max(cam_x-16,target.x+margin_left+shift_x);
    }
    
    
    // Vertical movement
    if (target.landed &amp;&amp; target.y &gt;= minimum_y+center_y &amp;&amp; target.y &lt;= maximum_y-center_y) {ground_offset = cam_y - target.y}
    
    
    
    if (target.y&gt;=cam_y+margin_bottom-shift_y &amp;&amp; ver_lag = 0 &amp;&amp; !target.ground &amp;&amp; cam_y &lt; maximum_y-center_y){
        cam_y=min(cam_y+16,target.y-margin_bottom+shift_y );
    }
    if (target.y&lt;=cam_y-margin_top-shift_y &amp;&amp; ver_lag = 0 &amp;&amp; !target.ground &amp;&amp; cam_y &gt; minimum_y+center_y){
        cam_y=max(cam_y-16,target.y+margin_top+shift_y );
    }
    
    if (target.y&gt;=cam_y+0-shift_y &amp;&amp; ver_lag = 0 &amp;&amp; target.ground &amp;&amp; cam_y &lt; maximum_y-center_y){
        cam_y=min(cam_y+16,target.y-0+shift_y);
    }
    if (target.y&lt;=cam_y+0-shift_y  &amp;&amp; ver_lag = 0 &amp;&amp; target.ground &amp;&amp; cam_y &gt; minimum_y+center_y){
        cam_y=max(cam_y-16,target.y-0+shift_y );
    }
    
    if target.landed &amp;&amp; target.y &gt;= minimum_y+center_y &amp;&amp; target.y&lt;= maximum_y-center_y{cam_y=target.y;}
    
    //Camera bounds
    
    if cam_x &lt;= minimum_x+center_x {cam_x = min(cam_x+3,minimum_x+center_x)}
    if cam_x &gt;= maximum_x-center_x {cam_x = max(cam_x-3,maximum_x-center_x)}
    if cam_y &lt;= minimum_y+center_y {cam_y = min(cam_y+3,minimum_y+center_y)}
    if cam_y &gt;= maximum_y-center_y {cam_y = max(cam_y-3,maximum_y-center_y)}
    
    //Ground offset
    
   if cam_y &gt;= minimum_y+center_y &amp;&amp; ground_offset &lt;=0 &amp;&amp; target.y &lt;= maximum_y-center_y {ground_offset = min(ground_offset+6,0)}
   if cam_y &gt;= minimum_y+center_y &amp;&amp; ground_offset &gt;=0 &amp;&amp; target.y &lt;= maximum_y-center_y {ground_offset = max(ground_offset-8,0)}
    
    
    
    //Set position
    x = floor(cam_x);
    y = floor(cam_y+ground_offset+8);
}
</string>
          </argument>
        </arguments>
      </action>
